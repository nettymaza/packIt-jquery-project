<h1>Log in</h1>

<%= form_for(:session, url: login_path) do |f| %>

    <%= f.label :email %>
    <%= f.email_field :email %>
    <br>

    <%= f.label :password %>
    <%= f.password_field :password %>
    <br>

    <%= f.submit "Log in"%>
<% end %>

<div class="links">
    <br>
    <div>
        <%= link_to('Login via Github', '/auth/github') %>
    </div>
    <br>
    <div>
        <%= link_to "Create Account", signup_path %>
    </div>
</div>
