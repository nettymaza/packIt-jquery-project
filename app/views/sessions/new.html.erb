<h1>Log in</h1>

<%= form_for(:session, url: login_path) do |f| %>

    <%= f.label :email %>
    <%= f.email_field :email %>
    <br>

    <%= f.label :password %>
    <%= f.password_field :password %>
    <br>

    <%= f.submit "Log in"%>
<% end %>

<p><%= link_to('Login via Github', '/auth/github') %></p>


<p>New user? <%= link_to "Sign up now!", signup_path %></p>
